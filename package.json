{
  "name": "tododeloo-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "node_modules/.bin/ng",
    "start": "node_modules/.bin/ng serve",
    "ssr": "npm run build && node dist/server.js",
    "build": "run-s build:client build:aot build:server",
    "build:client": "node_modules/.bin/ng build -prod --build-optimizer --app 0",
    "build:aot": "node_modules/.bin/ng build --aot --app 1",
    "build:server": "node_modules/.bin/webpack -p",
    "test": "node_modules/.bin/ng test",
    "lint": "node_modules/.bin/ng lint",
    "e2e": "node_modules/.bin/ng e2e",
    "api": "run-p api:meta api:default",
    "api:default": "node_modules/.bin/ng-swagger-gen --config tododeloo.api.json",
    "api:meta": "node_modules/.bin/ng-swagger-gen --config tododeloo.meta.api.json",
    "sonar": "run-s coverage sonar:inspect",
    "coverage": "node_modules/.bin/ng test --single-run --watch false --code-coverage",
    "sonar:inspect": "node_modules/.bin/sonar-scanner",
    "compodoc": "node_modules/.bin/compodoc -p tsconfig.json",
    "update:cli": "rm -rf node_modules dist && npm install --save-dev @angular/cli@latest && npm install",
    "nsp": "run-p nsp:json nsp:check",
    "nsp:json": "node_modules/.bin/nsp check --reporter json > nsp.json",
    "nsp:check": "node_modules/.bin/nsp check"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.5",
    "@angular/common": "^5.2.5",
    "@angular/compiler": "^5.2.5",
    "@angular/core": "^5.2.5",
    "@angular/forms": "^5.2.5",
    "@angular/http": "^5.2.5",
    "@angular/platform-browser": "^5.2.5",
    "@angular/platform-browser-dynamic": "^5.2.5",
    "@angular/platform-server": "^5.2.5",
    "@angular/router": "^5.2.5",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.8",
    "@angular/compiler-cli": "^5.2.5",
    "@angular/language-service": "^5.2.5",
    "@compodoc/compodoc": "^1.0.6",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.101",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-swagger-gen": "^0.10.0",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.2.1",
    "protractor": "~5.2.0",
    "sonar-scanner": "^3.0.3",
    "ts-loader": "^3.5.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "webpack": "^3.11.0",
    "webpack-node-externals": "^1.6.0"
  }
}
